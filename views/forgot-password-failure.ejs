<title>Kopa Booka | Reset Password Failure</title>
</head>
<body>

	<%- include("partials/header"); -%>

	<section class="messageBox-cont">
		<div class="message-box" style="width: 800px;">
			<div class="messageTitle-cont">
				<i class="fa fa-times"></i>
				<h1 class="messageTitle">Something went wrong...</h1>
			</div>

			<div class="message-cont">
				<p class="message">Sorry, Our system failed to send the reset password link. 
				Please click the button below to return to the forgot password page and try again.</p>
			</div>

			<div class="messageBoxRedirect-cont">
				<a href="/forgot-password" class="messageRedirect">Go back to Forgot-Password page.</a>
			</div>
		</div>
	</section>
	
	<%- include("partials/footer"); -%>
	
  <script src="/Post-Ad-Page-JS/Post Ad.js"></script>
</body>
